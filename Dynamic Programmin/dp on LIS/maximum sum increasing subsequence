class Solution{
		

	public:
	int maxSumIS(int arr[], int n)  
	{  
	    vector<int>dp(n);
	    
	    for(int i=0; i<n; i++)
	        dp[i] = arr[i];
	    
	    for(int i=0; i<n; i++){
	        for(int j=0; j<i; j++){
	            if(arr[j] < arr[i] && (arr[i] + dp[j] > dp[i])){
	                dp[i] = arr[i] + dp[j];
	            }
	        }
	    }
	    
	    int sum = INT_MIN;
	    for(int i=0; i<n; i++){
	        sum = max(sum, dp[i]);
	    }
	    return sum;
	}  
};
