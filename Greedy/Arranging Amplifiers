#include <iostream>
using namespace std;

int main() {
	
	// your code here
	int t;
	cin>>t;
	while(t--){
		int n;
		cint>>n;
		int arr[n];
		int ones =0;
		for(int i=0; i<n; i++){
			cin>>arr[i]<<" ";
			if(arr[i] == 1) ones++;
		}
		sort(arr, arr+n, greater<int>());
		
		for(int i=0; i<ones; i++){
			cout<<1<<endl;
		}
		
		if(n-ones == 2) and arr[0] == 2 and arr[1] == 3{
			cout<<2<<" "<<3<<endl;
		}
		
		else{
				for(int i=0; i<n; i++){
					cout<<arr[i]<<endl;
				}
		}
	 return 0;
			
	}

	return 0;
}
