#include <iostream>

using namespace std;

int main()
{
    string s;
    cin>>s;
    
    string word = "hello";
    int i=0, j=0;
    while(j < s.length()){
        if(word[i] == s[j]){
            i++;
            j++;
        }
        else{
            j++;
        }
    }
    if(i < word.length()){
            cout<<"NO";
        }
        else{
            cout<<"YES";
        }
    //cout<<i;
    
    return 0;
}
