#include <iostream>
#include<unordered_map>
#include<string>
using namespace std;

int main()
{
    int n;
    cin>>n;
    
    unordered_map<string, string>ump;
    
    ump["purple"] = "Power";
    ump["green"] = "Time";
    ump["blue"] = "Space";
    ump["orange"] = "Soul";
    ump["red"] = "Reality";
    ump["yellow"] = "Mind";
    
    string s;
    for(int i=0; i<n; i++){
        cin>>s;
        ump.erase(s);
    }
    cout<<ump.size()<<endl;
    for(auto i=ump.begin(); i!=ump.end(); i++){
        cout<<i->second<<endl;
    }

    return 0;
}
