#include <iostream>

using namespace std;

int main()
{
   int number_of_students, number_of_pens, number_of_notebook;
   cin>>number_of_students>>number_of_pens>>number_of_notebook;
   
   if((number_of_pens < number_of_students) || (number_of_notebook < number_of_students)){
       cout<<"No"<<endl;
   }
   else{
       cout<<"Yes"<<endl;
   }

    return 0;
}
