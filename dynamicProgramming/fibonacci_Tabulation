#include <iostream>
#include<bits/stdc++.h>
using namespace std;

//using tabulation
int fiboTablu(int n, vector<int>&dp){
    dp[0] = 0;
    dp[1] = 1;
    
    for(int i=2; i<=n; i++){
        dp[i] = dp[i-1] + dp[i-2];
    }
    return dp[n];
}
int main()
{
    cout<<"Enter the nth number: "<<endl;
    int n;
    cin>>n;
   //memoization
   //Step 1: declaration of dp array
   vector<int>dp(n+1, -1);
    cout<<"nth fibonacci number: "<<endl;
    cout<<fiboTablu(n, dp);

    return 0;
}
